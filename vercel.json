{
        "routes": [
          {
            "src": "/src/component/dist/(.*)",
            "dest": "/src/component/dist/$1",
            "headers": {
              "Cache-Control": "public, max-age=31536000, immutable"
            },
            "continue": true
          },
          {
            "src": "/(.*)",
            "dest": "/index.html"
          }
        ]
      }